#!/bin/sh

LUAROCKS_SYSCONFDIR='/usr/local/etc/luarocks' LUA_INIT='package.path="/root/Server_test/bundle/share/lua/5.3/?.lua;/root/Server_test/bundle/share/lua/5.3/?/init.lua;"..package.path;package.cpath="/root/Server_test/bundle/lib/lua/5.3/?.so;"..package.cpath;local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("moonscript","0.5.0-1")' exec '/root/Server_test/skynet/3rd/lua/lua' '/root/Server_test/bundle/lib/luarocks/rocks-5.3/moonscript/0.5.0-1/bin/moonc' "$@"
